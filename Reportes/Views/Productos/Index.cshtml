
@{
    ViewBag.Title = "Index";
}

<h2>Lista de productos</h2>
<div class="container">
    <div class="col-md-12">
        <table cellpadding="2" cellspacing="2" border="1">
            <tr>
            <th>id</th>
            <th>nombre</th>
            <th>precio</th>
            </tr>
            @foreach(var productos in ViewBag.productos)
            {
                <tr>
                    <td>@productos.id</td>
                    <td>@productos.nombre</td>
                    <td>@productos.precio</td>
                    <td><a href="@Url.Action("exportar","Productos")">HOLA</a></td>
                </tr>
            }
        </table>
        <br />
    </div>

</div>

